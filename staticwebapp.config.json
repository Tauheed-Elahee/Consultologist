{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/*.{css,scss,sass,js,json,ico,svg,png,jpg,jpeg,gif,webp,woff,woff2,ttf,eot,otf}"]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html",
      "statusCode": 404
    }
  },
  "platform": {
    "apiRuntime": "node:20"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".liquid": "text/plain"
  }
}
